{
  "name": "firestarter",
  "version": "0.1.0",
  "private": true,
  "proxy": "http://localhost:5000",
  "dependencies": {
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-scripts": "1.0.14",
    "redux-thunk": "^2.2.0",
    "uuid": "^3.1.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "local-server": "firebase serve --only functions,hosting",
    "upgrade-local-emulator": "npm uninstall -g @google-cloud/functions-emulator && npm install -g @google-cloud/functions-emulator && open https://firebase.google.com/docs/functions/local-emulator",
    "test-firebase": "open https://firebase.google.com/docs/functions/unit-testing",
    "test-fb": "mocha functions/test --reporter spec",
    "babel": "babel src/ -d functions/dist/ --presets es2015,stage-2,react",
    "postbuild": "copyfiles -f build/**/*.css build/asset-manifest.json functions/dist",
    "webpack": "npm run build && rm build/index.html && webpack --config webpack.config.ssr.js"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-css-modules-transform": "^1.2.7",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "copyfiles": "^1.2.0",
    "css-loader": "^0.28.7",
    "extract-text-webpack-plugin": "^3.0.0",
    "firebase-admin": "^5.4.3",
    "firebase-functions": "^0.7.1",
    "mocha": "^3.5.0",
    "react-redux": "^5.0.6",
    "react-svg-loader": "^1.1.1",
    "redux": "^3.7.2",
    "regress": "^1.0.2",
    "reqres": "^1.3.0",
    "sinon": "^3.2.1",
    "svg-inline-loader": "^0.8.0",
    "to-string-loader": "^1.1.5",
    "webpack": "^3.6.0"
  }
}
